{
  "name": "Tysonize",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "19": "mike-19.png",
      "38": "mike-38.png"
    },
    "default_title": "Tysonize this page!!"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["content.js"]
    }
  ]
}
